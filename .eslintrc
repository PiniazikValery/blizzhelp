{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-underscore-dangle":"off",
      "max-len":"off",
      "react/jsx-filename-extension": "off",
      "quote-props":"off",
      "react/prefer-stateless-function": "off"
    }
  }